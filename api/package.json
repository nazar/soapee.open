{
  "name": "api",
  "version": "1.0.0",
  "description": "Soapee API Server",
  "main": "index.js",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "start": "webpack --config ./webpack.dev.js --watch & nodemon -L --watch dev --watch config dev/index.js & wait",
    "copy:config": "cp -r config dist && cp package.json dist && cp yarn.lock dist",
    "build": "rm -rf dist/* && webpack --config ./webpack.prod.js && yarn copy:config",
    "lint": "yarn eslint .",
    "test": "NODE_ENV=test mocha --require @babel/register --recursive --exit --check-leaks ./test/scope",
    "coverage": "NODE_ENV=test nyc mocha --require @babel/register --recursive --exit ./test/scope"
  },
  "author": "Nazar Aziz <mcnazar@gmail.com>",
  "license": "AGPL-3.0-or-later",
