    "bcrypt": "5.0.1",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "branchy": "1.0.0",
    "compression": "1.7.4",
    "config": "3.3.6",
    "cookie-parser": "1.4.5",
    "dataloader": "2.0.0",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "express-graphql": "0.12.0",
    "express-http-context": "1.2.4",
    "get-folder-size": "2.0.1",
    "graphql": "15.5.1",
